    "homepage": "https://github.com/carsonarrow/silverstripe-weservimage",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Carson Arrowsmith",
            "homepage": "https://www.carsonarrowsmith.com"
        }
    ],
    "support": {
        "issues": "https://github.com/carsonarrow/silverstripe-weservimage/issues"
    },
    "require": {
